set term qt font " DejaVu Math TeX Gyre, 20" size 800, 600

set xtics (3, 6, 9, 12, 15)
set ytics (0.6, 0.9, 1.2)
set xtics nomirror
set ytics nomirror

set xrange [2:15]
set yrange [0.55:1.4]

set key font "DejaVu Math TeX Gyre, 24"
set tics font "DejaVu Math TeX Gyre, 26"

set xlabel "Distance (x10^{-10} m)" font "DejaVu Math TeX Gyre, 30"
set ylabel "g(r)" font "DejaVu Math TeX Gyre, 30"
